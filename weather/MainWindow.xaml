<Window x:Class="weather.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:weather"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="degreeMax" Content="{Binding Path=main.temp_max}" HorizontalAlignment="Left" VerticalAlignment="Top" Height="72" Width="110" Margin="24,28,0,0"/>
        <Label x:Name="degreeMin" Content="{Binding Path=main.temp_min}" HorizontalAlignment="Left" VerticalAlignment="Top" Height="72" Width="110" Margin="24,132,0,0"/>
        <Label x:Name="baseData" Content="{Binding base}" HorizontalAlignment="Left" VerticalAlignment="Top" Height="72" Width="110" Margin="24,237,0,0"/>

    </Grid>
</Window>
